{
  "name": "@pledger-finance/contracts",
  "version": "0.0.1",
  "license": "GPL-3.0-only",
  "description": "Smart contracts for Pledger Finance",
  "author": "Justin Gao <flyingforce@gmail.com>",
  "scripts": {
    "remix": "remixd -s ./ https://remix.ethereum.org/",
    "compile": "npx buidler compile",
    "build": "npm run compile && npx buidler typechain",
    "sandbox:build": "cd sandbox && ./rebuild.sh",
    "clean": "rm -Rf ./typechain && rm -Rf ./build",
    "test": "npx buidler test",
    "test:local": "DOTENV_CONFIG_PATH=.env.local npx buidler test",
    "test:gas": "gasprofile/rungas.sh",
    "test:gas:all": "export ALL_TESTS=true; gasprofile/rungas.sh",
    "test:coverage": "export COVERAGE=true; npx buidler coverage --network coverage",
    "docs:build": "yarn run build && yarn run solidity-docgen -e cache/solpp-generated-contracts/lib,cache/solpp-generated-contracts/interface,cache/solpp-generated-contracts/upgradeable,cache/solpp-generated-contracts/storage,cache/solpp-generated-contracts/utils -i cache/solpp-generated-contracts -o ../../docs/contracts -t docs -H docs/helpers.js",
    "upgrade:kovan": "DEBUG=* DOTENV_CONFIG_PATH=.env.kovan ts-node scripts/upgrade.ts",
    "upgrade:rinkeby": "DEBUG=* DOTENV_CONFIG_PATH=.env.rinkeby ts-node scripts/upgrade.ts",
    "upgrade:mainnet": "DEBUG=* DOTENV_CONFIG_PATH=.env.mainnet ts-node scripts/upgrade.ts",
    "patch": "git format-patch master --relative --stdout > patch.diff",
    "sloc": "rename 's/\\.sol/\\.c/' contracts/*.sol && scc contracts/* --wide --by-file && rename 's/\\.c/\\.sol/' contracts/*.c",
    "deploy:bsctest:all": "DEBUG=* DOTENV_CONFIG_PATH=.env.bsc ts-node scripts/deploy.ts",
    "verify:bsctest:all": "DEBUG=* DOTENV_CONFIG_PATH=.env.bsc ts-node scripts/verify.ts",
    "deploy:bsc-mainnet:all": "DEBUG=* DOTENV_CONFIG_PATH=.env.bsc_mainnet ts-node scripts/deploy.ts",
    "verify:bsc-mainnet:all": "DEBUG=* DOTENV_CONFIG_PATH=.env.bsc_mainnet ts-node scripts/verify.ts",
    "verify:bsc-mainnet:cash": "DEBUG=* DOTENV_CONFIG_PATH=.env.bsc_mainnet ts-node scripts/verify1.ts",
    "setupLiquidity:bsc-mainnet": "DEBUG=* DOTENV_CONFIG_PATH=.env.bsc_mainnet ts-node scripts/setupLiquidity.ts",
    "removeLiquidity:bsc-mainnet": "DEBUG=* DOTENV_CONFIG_PATH=.env.bsc_mainnet ts-node scripts/removeLiquidity.ts",
    "setupLiquidity:bsctest": "DEBUG=* DOTENV_CONFIG_PATH=.env.bsc ts-node scripts/setupLiquidity.ts",
    "removeLiquidity:bsctest": "DEBUG=* DOTENV_CONFIG_PATH=.env.bsc ts-node scripts/removeLiquidity.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notional-finance/notional.git"
  },
  "homepage": "https://notional.finance",
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.8",
    "@nomiclabs/buidler-ethers": "^1.1.2",
    "@nomiclabs/buidler-etherscan": "^2.1.0",
    "@nomiclabs/buidler-ganache": "^1.1.2",
    "@nomiclabs/buidler-solhint": "^1.0.2",
    "@nomiclabs/buidler-solpp": "^1.1.1",
    "@nomiclabs/buidler-waffle": "^1.2.0",
    "@typechain/ethers-v4": "^1.0",
    "@typechain/truffle-v4": "^2.0",
    "@typechain/truffle-v5": "^2.0",
    "@typechain/web3-v1": "^1.0",
    "@types/chai": "^4.2.14",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.2",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "buidler-abi-exporter": "^1.0.1",
    "buidler-gas-reporter": "^0.1.3",
    "buidler-typechain": "^0.1",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^2.4.0",
    "ethlint": "^1.2.5",
    "ganache-cli": "^6.9.0",
    "graphql-request": "^1.8.2",
    "prettier": "^2.0.4",
    "prettier-plugin-solidity": "^1.0.0-alpha.38",
    "pretty-quick": "^2.0.1",
    "solidity-coverage": "^0.7.2",
    "solidity-docgen": "^0.5.4",
    "ts-generator": "0.0.8",
    "ts-node": "^8.10.2",
    "typechain": "^2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.1.0",
    "builder": "^5.0.0",
    "debug": "^4.1.1",
    "ethers": "^4.0.46"
  }
}
